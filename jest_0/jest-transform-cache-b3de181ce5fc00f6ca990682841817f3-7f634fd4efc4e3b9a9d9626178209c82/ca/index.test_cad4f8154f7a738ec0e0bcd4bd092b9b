"use strict";

var _src = require("../src");

describe('Index', function () {
  describe('fromNumber', function () {
    test('Debería convertir número a romano - Letra I', function () {
      expect((0, _src.fromNumber)(1)).toBe('I');
      expect((0, _src.fromNumber)(2)).toBe('II');
      expect((0, _src.fromNumber)(3)).toBe('III');
    });
    test('Debería convertir número a romano - Letra V', function () {
      expect((0, _src.fromNumber)(4)).toBe('IV');
      expect((0, _src.fromNumber)(5)).toBe('V');
    });
    test('Debería convertir número a romano - Letra X', function () {
      expect((0, _src.fromNumber)(9)).toBe('IX');
      expect((0, _src.fromNumber)(10)).toBe('X');
      expect((0, _src.fromNumber)(11)).toBe('XI');
    });
    test('Debería convertir número a romano - Letra Extra', function () {
      expect((0, _src.fromNumber)(2018)).toBe('MMXVIII');
      expect((0, _src.fromNumber)(2648)).toBe('MMDCXLVIII');
    });
    test('Caso especial de 0', function () {
      expect(function () {
        return (0, _src.fromNumber)(0);
      }).toThrow();
    });
    test('Caso especial de no number', function () {
      expect(function () {
        return (0, _src.fromNumber)('No number');
      }).toThrow();
    });
  });
});

exports.fromNumber = void 0;
var ROMAN_TABLE = {
  1000: "M",
  900: "CM",
  500: "D",
  400: "CD",
  100: "C",
  90: "XC",
  50: "L",
  40: "XL",
  10: "X",
  9: "IX",
  5: "V",
  4: "IV",
  1: "I"
};
var ROMAN_TABLE_KEYS = Object.keys(ROMAN_TABLE).reverse();

var fromNumber = function fromNumber(value) {
  if (typeof value !== 'number' || value < 0 || !Number.isInteger(value)) {
    throw new Error('No number');
  }

  var result = '';
  ROMAN_TABLE_KEYS.forEach(function (key) {
    while (value >= key) {
      value -= key;
      result += ROMAN_TABLE[key];
    }
  });
  return result;
};

exports.fromNumber = fromNumber;
